{
	"canisters": {
		"internet_identity": {
			"type": "custom",
			"candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
			"wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
			"shrink": true,
			"remote": {
				"id": {
					"ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
				}
			}
		},
		"ui": {
			"dependencies": [],
			"frontend": {
				"entrypoint": "build/index.html"
			},
			"source": ["build"],
			"gzip": true,
			"optimize": "cycles",
			"type": "assets"
		},
		"explore": {
			"main": "src/actor_explore/Explore.mo",
			"type": "motoko",
			"dependencies": ["logger"]
		},
		"logger": {
			"main": "src/actor_logger/Logger.mo",
			"type": "motoko",
			"dependencies": []
		},
		"creator": {
			"main": "src/actor_creator/Creator.mo",
			"type": "motoko",
			"dependencies": ["logger", "explore", "mo"]
		},
		"mo": {
			"main": "src/actor_mo/MO.mo",
			"type": "motoko",
			"dependencies": ["logger"]
		},
		"username_registry": {
			"main": "src/actor_username_registry/UsernameRegistry.mo",
			"type": "motoko",
			"dependencies": ["logger", "explore", "mo"]
		},
		"file_storage": {
			"main": "src/actor_file_storage/FileStorage.mo",
			"type": "motoko",
			"dependencies": ["logger", "mo"]
		},
		"file_scaling_manager": {
			"main": "src/actor_file_storage/FileScalingManager.mo",
			"type": "motoko",
			"dependencies": ["logger", "mo"]
		}
	},
	"defaults": {
		"build": {
			"packtool": "mops sources"
		}
	},
	"networks": {
		"local": {
			"bind": "127.0.0.1:8080"
		},
		"staging": {
			"providers": ["https://icp0.io"],
			"type": "persistent"
		}
	},
	"dfx": "0.24.3",
	"version": 1
}
