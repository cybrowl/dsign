{
  "private": true,
  "name": "dsign",
  "version": "0.5.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "generate_test_interface": "bash ./scripts/generate_can_test_interface.sh",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "logs": "npm run generate_test_interface && tape test-utils/debug/debug.test.cjs",
    "preview": "vite preview",
    "u-p": "sh ./scripts/deploy_test_cans/profile.sh && node ./scripts/upgrade_child_cans/profile.cjs",
    "u-pm": "sh ./scripts/deploy_test_cans/project_main.sh && node ./scripts/upgrade_child_cans/project_main.cjs",
    "u-sm": "sh ./scripts/deploy_test_cans/snap_main.sh && node ./scripts/upgrade_child_cans/snap_main.cjs",
    "test-all": "bash ./scripts/test-all.sh",
    "test-config": "tape test-env/dfx.config.test.cjs",
    "test-ex": "npm run generate_test_interface && tape test-canisters/explore.test.cjs",
    "test-ta": "npm run generate_test_interface && tape test-canisters/test_assets.test.cjs",
    "test-sm": "npm run generate_test_interface && tape test-canisters/snap_main.test.cjs",
    "test-p": "npm run generate_test_interface && tape test-canisters/profile.test.cjs",
    "test-pm": "npm run generate_test_interface && tape test-canisters/project_main.test.cjs",
    "test-u": "npm run generate_test_interface && tape test-canisters/username.test.cjs",
    "watch": "vite dev"
  },
  "dependencies": {
    "@dfinity/agent": "^0.14.1",
    "@dfinity/auth-client": "^0.14.1",
    "@dfinity/candid": "^0.14.1",
    "@dfinity/identity": "^0.14.1",
    "@dfinity/principal": "^0.14.1",
    "dsign-components": "^0.17.1",
    "lodash": "^4.17.21",
    "svelte-local-storage-store": "^0.2.6"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.48",
    "@sveltejs/kit": "next",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fake-words": "^0.5.3",
    "node-fetch": "^2.6.7",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "prettier": "^2.8.0",
    "prettier-plugin-motoko": "^0.2.3",
    "prettier-plugin-svelte": "^2.8.1",
    "rollup-plugin-terser": "^7.0.2",
    "sha256": "^0.2.0",
    "svelte": "^3.52.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.4",
    "tap-nyan": "^1.1.0",
    "tape": "^5.6.1",
    "vite": "^3.2.4"
  }
}
